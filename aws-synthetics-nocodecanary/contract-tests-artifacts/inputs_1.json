{
  "CreateInputs": {
    "Name": "test-no-code-canary",
    "Schedule": {
      "Expression": "rate(1 minute)",
      "DurationInSeconds": "3600"
    },
    "EndpointList": [
      "https://amazon.com"
    ],
    "NoCodeCanaryConfig": {
      "HttpConfig": {
        "Port": 443,
        "HttpMethod": "GET",
        "UseIPv6": true,
        "VerifyCert": true
      }
    },
    "Tags": [
      {
        "Key": "key001",
        "Value": "value001"
      }
    ],
    "StartCanaryAfterCreation": false
  },
  "PatchInputs": [
    {
      "op": "replace",
      "path": "EndpointList",
      "value": [
        "https://aws.amazon.com"
      ]
    }
  ]
}

